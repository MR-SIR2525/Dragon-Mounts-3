{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.dragon_fly_conend": {
      "states": {
        "fly": {
          "transitions": [
            {
              "fly_back": "!query.has_rider"
            }
          ]
        },
        "fly_back": {
          "transitions": [
            {
              "fly": "!query.has_rider"
            }
          ],
          "on_exit": [
            "/replaceitem entity @s slot.weapon.mainhand 0 air 1"
          ]
        }
      }
    }
  }
}
